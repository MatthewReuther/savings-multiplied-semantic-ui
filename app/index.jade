          div.main_content_footer
            ul
              li icon - facebook
              li icon - twitter
              li icon - message boards
              li icon - special offer
          div.sub_content_footer
            ul
              li link here
              li link here
              li link here
              li link here
      div(class="two wide column")
        
      


    script(src="js/main.js")
